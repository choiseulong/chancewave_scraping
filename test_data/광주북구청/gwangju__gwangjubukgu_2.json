 : \ub098\ub9cc\uc758 \uc2ac\ub77c\uc784 \ub9cc\ub4e4\uae30/\ud2b9\ubcc4\ud55c \ubc43\uc9c0 \ub9cc\ub4e4\uae30\u203b \uccb4\ud5d8\ubd80\uc2a4\ub294 \ubc29\uc5ed\uc218\uce59\uc5d0 \ub530\ub77c \ucc38\uc5ec\uc778\uc6d0\uc774 \uc81c\ud55c\ub429\ub2c8\ub2e4.\u203b \ucc38\uc5ec\ubc29\ubc95 : \ud604\uc7a5\uc5d0\uc11c \uc120\ucc29\uc21c\uc73c\ub85c \uc811\uc218 \u2605 \uad00\ub78c \uc548\ub0b4\uc0ac\ud56d \u2605 \u2611\ufe0f\ubc31\uc2e0\uc811\uc885\uc644\ub8cc\uc790\ub9cc \uc785\uc7a5\uac00\ub2a5\ud569\ub2c8\ub2e4.(18\uc138 \uc774\ud558\ub294 \uc785\uc7a5 \uac00\ub2a5)\u2611\ufe0f\ud604\uc7a5\uc5d0\uc11c \ub3d7\uc790\ub9ac\ub97c \ube4c\ub824\ub4dc\ub824\uc694(\uc9d1\uc5d0\uc11c \uac00\uc838\uc624\uc154\ub3c4 \uc88b\uc544\uc694\u263a\ufe0f)\u2611\ufe0f\uc6b0\ucc9c \uc2dc \ud589\uc0ac\ub294 \uc790\ub3d9 \ucde8\uc18c\ub429\ub2c8\ub2e4.\u2611\ufe0f\uc77c\ubab0\uc2dc\uac04\uc5d0 \ub530\ub77c \ud589\uc0ac\uc2dc\uac04\uc774 \uc870\uc815\ub420 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\u2611\ufe0f\ubaa8\ub450\uc758 \uc548\uc804\uc744 \uc704\ud574 \ubc29\uc5ed\uc218\uce59\uc744 \uc9c0\ud0a4\uba70 \uc990\uaca8\uc8fc\uc138\uc694.(\ub9c8\uc2a4\ud06c \ucc29\uc6a9, 1m\uc774\uc0c1 \uac70\ub9ac\ub450\uae30, \uad11\uc7a5 \ub0b4 \ucde8\uc2dd\uae08\uc9c0)", "contact": [], "post_image_url": [], "channel_location_name": "\uad11\uc8fc\uad11\uc5ed\uc2dc", "full_channel_code": "gwangju__gwangjubukgu_2", "channel_name": "\uad11\uc8fc\ubd81\uad6c\uccad", "post_board_name": "\ubb38\ud654\ud589\uc0ac", "channel_code": "gwangjubukgu_2", "channel_url": "https://bukgu.gwangju.kr/board.es?mid=a10201110000&bid=0315&nPage=1", "post_url_can_use": true, "created_time": "2022-02-15T09:58:59.735720+09:00", "post_text_type": "only_post_text"}
