758 60% \ubbf8\ub9cc \uc2e0\uccad\ub41c \uacfc\ubaa9\uc740 \uac15\uc88c \uac1c\uc124\uc774 \ucde8\uc18c \ub420\uc218 \uc788\uc2b5\ub2c8\ub2e4. \u274d \uc6d4\ubcc4 \ucd9c\uc11d\ub960 60% \ubbf8\ub9cc \uacfc\ubaa9\uc740 \uad50\uc721\uae30\uac04\uc911\uc5d0\ub3c4 \ud3d0\uac15\ub420 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \u274d \ub2e8\uc21c \ubcc0\uc2ec\uc5d0 \uc758\ud55c \uc218\uac15\ucde8\uc18c\u2024\uacb0\uc11d\uc73c\ub85c \uad50\uc721 \uc6b4\uc601\uc5d0 \uc9c0\uc7a5\uc744 \ucd08\ub798\ud558\ub294 \uacbd\uc6b0 \ub2e4 \uc74c \ud559\uae30 \uc218\uac15 \uc2e0\uccad\uc5d0 \uc81c\ud55c\uc744 \ubc1b\uc744 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \u274d \uac15\uc758 \uc694\uc77c \ubc0f \uc2dc\uac04\uc740 \uc0ac\uc815\uc5d0 \ub530\ub77c \uc77c\ubd80 \ubcc0\uacbd\ub420 \uc218 \uc788\uc73c\uba70, \uc77c\ubd80 \uacfc\ubaa9\uc740 \ubaa8\uc9d1\uc815 \uc6d0 \ucd08\uacfc\ub85c \uc870\uae30\uc5d0 \ub9c8\uac10\ub420 \uc218 \uc788\uc2b5\ub2c8\ub2e4.", "contact": [], "post_image_url": [], "channel_location_name": "\uac15\uc6d0\ub3c4", "full_channel_code": "gangwon__taebaek_0", "channel_name": "\ud0dc\ubc31\uc2dc\uccad", "post_board_name": "\uc0c8\uc18c\uc2dd", "channel_code": "taebaek_0", "channel_url": "http://www.taebaek.go.kr/www/selectBbsNttList.do?key=351&bbsNo=24&pageUnit=10&searchCnd=all&pageIndex=1", "post_url_can_use": true, "created_time": "2022-02-15T10:07:04.977509+09:00", "post_text_type": "only_post_text"}
